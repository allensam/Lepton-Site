<div id="ng-app" data-module="htmlModule">
  <div ng-controller="htmlBind" id="htmlBind">
    <div change-font-size>
    </div>
    <span ng-bind-html="htmlExample"></span>
    <br>
    <br>
    <!-- editor that binds html to page -->
    <div class="bind-html" id="htmlExample" onkeyup="keyUpEventListener(this);">
    </div>
  </div>
</div>
<%= javascript_include_tag 'compile-routes/super-compile-route', 'data-turbolinks-track' => true %>
<%= javascript_include_tag 'editors/html-page', 'data-turbolinks-track' => true %>
